<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
</head>
<body>
    <%- include('./partials/navbar') %>
    <%- JSON.stringify(blogs)  %>

    <% blogs.forEach(blog => { %>
        <div class="card" style="width: 18rem;">
            <img src="<%= blog.coverImage  %>" class="card-img-top" alt="...">
            <div class="card-body">
              <p class="card-text"><%= blog.content %></p>
            </div>
          </div>
    <% }) %>

    <%- include('./partials/script') %>
    
</body>
</html>